{
  "name": "sdk-poc",
  "version": "1.0.0",
  "description": "Chorme extension poc",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build:prod": "webpack --config config/webpack.config.js --env prod --mode=production",
    "build:local": "webpack --config config/webpack.config.js --env local --mode=development",
    "build:dev": "webpack --config config/webpack.config.js --env dev --mode=development",
    "build:staging": "webpack --config config/webpack.config.js --env staging --mode=production"
  },
  "devDependencies": {
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "4.3.0",
    "file-loader": "6.2.0",
    "replace-in-file-webpack-plugin": "1.0.6",
    "size-plugin": "2.0.2",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-merge": "5.7.3"
  }
}
